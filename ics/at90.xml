                <set code="CKSEL=0010 SUT=01" text="Int. RC Osc.; Start-up time: 6 CK + 4.1 ms" />
                <set code="CKSEL=0010 SUT=10" text="Int. RC Osc.; Start-up time: 6 CK + 65 ms" />

                <!-- Boot start address will be added to chip offset -->
                <!--chip="AT90CAN32: $3C00, AT90CAN64: $7C00, AT90CAN128: $FC00" -->
                <set code="BOOTSZ=10" text="Boot Flash section size=1024 words Boot start address=$nC00" />
                <!--chip="AT90CAN32: $3800, AT90CAN64: $7800, AT90CAN128: $F800" -->
                <set code="BOOTSZ=01" text="Boot Flash section size=2048 words Boot start address=$n800" />
                <!--chip="AT90CAN32: $3000, AT90CAN64: $7000, AT90CAN128: $F000" -->
                <set code="BOOTSZ=00" text="Boot Flash section size=4096 words Boot start address=$n000" />
                <!--chip="AT90CAN32: $3E00, AT90CAN64: $7E00, AT90CAN128: $FE00" -->
                <set code="BOOTSZ=11" text="Boot Flash section size=512 words Boot start address=$nE00" />

                <set code="BODLEVEL=111" text="Brown-out detection disabled" />
                <set code="BODLEVEL=000" text="Brown-out detection level at VCC=2.5 V" />
                <set code="BODLEVEL=001" text="Brown-out detection level at VCC=2.6 V" />
                <set code="BODLEVEL=010" text="Brown-out detection level at VCC=2.7 V" />
                <set code="BODLEVEL=011" text="Brown-out detection level at VCC=3.8 V" />
                <set code="BODLEVEL=100" text="Brown-out detection level at VCC=3.9 V" />
                <set code="BODLEVEL=101" text="Brown-out detection level at VCC=4.0 V" />
                <set code="BODLEVEL=110" text="Brown-out detection level at VCC=4.1 V" />
            </fuse>
            <lock>
                <bit offset="0" name="Lock" index="0" />
                <bit offset="1" name="Lock" index="1" />
                <bit offset="2" name="BootLock" index="0" />
                <bit offset="3" name="BootLock" index="1" />
                <bit offset="4" name="BootLoad" index="0" />
                <bit offset="5" name="BootLoad" index="1" />

                <!-- lock sets description -->
                <set code="Lock=11" text="Mode 1: No memory lock features enabled" />
                <set code="Lock=10" text="Mode 2: Further programming disabled" />
                <set code="Lock=00" text="Mode 3: Further programming and verification disabled" />

                <set code="BootLock=11" text="Application Protection Mode 1: No lock on SPM and LPM in Application Section" />
                <set code="BootLock=10" text="Application Protection Mode 2: SPM prohibited in Application Section" />
                <set code="BootLock=00" text="Application Protection Mode 3: LPM and SPM prohibited in Application Section" />
                <set code="BootLock=01" text="Application Protection Mode 4: LPM prohibited in Application Section" />

                <set code="BootLoad=11" text="Boot Loader Protection Mode 1: No lock on SPM and LPM in Boot Loader Section" />
                <set code="BootLoad=10" text="Boot Loader Protection Mode 2: SPM prohibited in Boot Loader Section" />
                <set code="BootLoad=00" text="Boot Loader Protection Mode 3: LPM and SPM prohibited in Boot Loader Section" />
                <set code="BootLoad=01" text="Boot Loader Protection Mode 4: LPM prohibited in Boot Loader Section" />
            </lock>
        </descr>
        <descr list="AT90S1200">
            <fuse>
                <bit offset="0" name="/FB0" text="NOT External clock enabled" />
                <bit offset="5" name="SPIEN" text="Serial program downloading (SPI) enabled" />
            </fuse>
            <lock>
                <bit offset="1" name="LB" index="0" />
                <bit offset="2" name="LB" index="1" />
