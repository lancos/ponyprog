        <!-- end of offset info -->
        <descr list="AT90CAN32,AT90CAN64,AT90CAN128">
            <fuse>
                <bit offset="0" name="CKSEL" index="0" />
                <bit offset="1" name="CKSEL" index="1" />
                <bit offset="2" name="CKSEL" index="2" />
                <bit offset="3" name="CKSEL" index="3" />
                <bit offset="4" name="SUT" index="0" />
                <bit offset="5" name="SUT" index="1" />
                <bit offset="6" name="CKOUT" text="Clock output on PORTC7" />
                <bit offset="7" name="CKDIV8" text="Divide clock by 8 internally" />
                <!-- byte high -->
                <bit offset="8" name="BOOTRST" text="Boot Reset vector Enabled (default address=$0000)" />
                <bit offset="9" name="BOOTSZ" index="0" />
                <bit offset="10" name="BOOTSZ" index="1" />
                <bit offset="11" name="EESAVE" text="Preserve EEPROM memory through the Chip Erase cycle" />
                <bit offset="12" name="WDTON" text="Watchdog timer always on" />
                <bit offset="13" name="SPIEN" text="Serial program downloading (SPI) enabled" />
                <bit offset="14" name="JTAGEN" text="JTAG Interface Enabled" />
                <bit offset="15" name="OCDEN" text="On-Chip Debug Enabled" />
                <!-- byte ext -->
                <bit offset="16" name="TA0SEL" text="Reserved for factory tests" />
                <bit offset="17" name="BODLEVEL" index="0" />
                <bit offset="18" name="BODLEVEL" index="1" />
                <bit offset="19" name="BODLEVEL" index="2" />

                <!-- fuse sets description -->
                <set code="CKSEL=0000 SUT=00" text="Ext. Clock; Start-up time: 6 CK + 0 ms" />
                <set code="CKSEL=0000 SUT=01" text="Ext. Clock; Start-up time: 6 CK + 4.1 ms" />
                <set code="CKSEL=0000 SUT=10" text="Ext. Clock; Start-up time: 6 CK + 65 ms" />
                <set code="CKSEL=1000 SUT=10" text="Ext. Crystal Osc.; Frequency 0.4-0.9 MHz; Start-up time: 1K CK + 0 ms" />
                <set code="CKSEL=1000 SUT=11" text="Ext. Crystal Osc.; Frequency 0.4-0.9 MHz; Start-up time: 1K CK + 4.1 ms" />
                <set code="CKSEL=1001 SUT=00" text="Ext. Crystal Osc.; Frequency 0.4-0.9 MHz; Start-up time: 1K CK + 65 ms" />
                <set code="CKSEL=1000 SUT=00" text="Ext. Crystal Osc.; Frequency 0.4-0.9 MHz; Start-up time: 258 CK + 4.1 ms" />
                <set code="CKSEL=1000 SUT=01" text="Ext. Crystal Osc.; Frequency 0.4-0.9 MHz; Start-up time: 258 CK + 65 ms" />
                <set code="CKSEL=1010 SUT=10" text="Ext. Crystal Osc.; Frequency 0.9-3.0 MHz; Start-up time: 1K CK + 0 ms" />
                <set code="CKSEL=1010 SUT=11" text="Ext. Crystal Osc.; Frequency 0.9-3.0 MHz; Start-up time: 1K CK + 4.1 ms" />
                <set code="CKSEL=1011 SUT=00" text="Ext. Crystal Osc.; Frequency 0.9-3.0 MHz; Start-up time: 1K CK + 65 ms" />
                <set code="CKSEL=1010 SUT=00" text="Ext. Crystal Osc.; Frequency 0.9-3.0 MHz; Start-up time: 258 CK + 4.1 ms" />
                <set code="CKSEL=1010 SUT=01" text="Ext. Crystal Osc.; Frequency 0.9-3.0 MHz; Start-up time: 258 CK + 65 ms" />
                <set code="CKSEL=1100 SUT=10" text="Ext. Crystal Osc.; Frequency 3.0-8.0 MHz; Start-up time: 1K CK + 0 ms" />
                <set code="CKSEL=1100 SUT=11" text="Ext. Crystal Osc.; Frequency 3.0-8.0 MHz; Start-up time: 1K CK + 4.1 ms" />
                <set code="CKSEL=1101 SUT=00" text="Ext. Crystal Osc.; Frequency 3.0-8.0 MHz; Start-up time: 1K CK + 65 ms" />
                <set code="CKSEL=1100 SUT=00" text="Ext. Crystal Osc.; Frequency 3.0-8.0 MHz; Start-up time: 258 CK + 4.1 ms" />
                <set code="CKSEL=1100 SUT=01" text="Ext. Crystal Osc.; Frequency 3.0-8.0 MHz; Start-up time: 258 CK + 65 ms" />
                <set code="CKSEL=1110 SUT=10" text="Ext. Crystal Osc.; Frequency 8.0-16.0 MHz; Start-up time: 1K CK + 0 ms" />
                <set code="CKSEL=1110 SUT=11" text="Ext. Crystal Osc.; Frequency 8.0-16.0 MHz; Start-up time: 1K CK + 4.1 ms" />
                <set code="CKSEL=1111 SUT=00" text="Ext. Crystal Osc.; Frequency 8.0-16.0 MHz; Start-up time: 1K CK + 65 ms" />
                <set code="CKSEL=1110 SUT=00" text="Ext. Crystal Osc.; Frequency 8.0-16.0 MHz; Start-up time: 258 CK + 4.1 ms" />
                <set code="CKSEL=1110 SUT=01" text="Ext. Crystal Osc.; Frequency 8.0-16.0 MHz; Start-up time: 258 CK + 65 ms" />
                <set code="CKSEL=0100 SUT=00" text="Ext. Low-Freq. Crystal; Start-up time: 1K CK + 0 ms" />
                <set code="CKSEL=0110 SUT=00" text="Ext. Low-Freq. Crystal; Start-up time: 1K CK + 0 ms; Int. Cap." />
                <set code="CKSEL=0100 SUT=01" text="Ext. Low-Freq. Crystal; Start-up time: 1K CK + 4.1 ms" />
                <set code="CKSEL=0110 SUT=01" text="Ext. Low-Freq. Crystal; Start-up time: 1K CK + 4.1 ms; Int. Cap." />
                <set code="CKSEL=0100 SUT=10" text="Ext. Low-Freq. Crystal; Start-up time: 1K CK + 65 ms" />
                <set code="CKSEL=0110 SUT=10" text="Ext. Low-Freq. Crystal; Start-up time: 1K CK + 65 ms; Int. Cap." />
                <set code="CKSEL=0101 SUT=00" text="Ext. Low-Freq. Crystal; Start-up time: 32K CK + 0 ms" />
                <set code="CKSEL=0111 SUT=00" text="Ext. Low-Freq. Crystal; Start-up time: 32K CK + 0 ms; Int. Cap." />
                <set code="CKSEL=0101 SUT=01" text="Ext. Low-Freq. Crystal; Start-up time: 32K CK + 4.1 ms" />
                <set code="CKSEL=0111 SUT=01" text="Ext. Low-Freq. Crystal; Start-up time: 32K CK + 4.1 ms; Int. Cap." />
                <set code="CKSEL=0101 SUT=10" text="Ext. Low-Freq. Crystal; Start-up time: 32K CK + 65 ms" />
                <set code="CKSEL=0111 SUT=10" text="Ext. Low-Freq. Crystal; Start-up time: 32K CK + 65 ms; Int. Cap." />
