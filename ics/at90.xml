                <set code="LB=11" text="Mode 1: No memory lock features enabled" />
                <set code="LB=10" text="Mode 2: Further programming disabled" />
                <set code="LB=00" text="Mode 3: Further programming and verification disabled" />
            </lock>
        </descr>
        <descr list="AT90S2313,AT90S4414,AT90S8515,AT90S4434,AT90S8535">
            <fuse>
                <bit offset="0" name="FSTRT" text="Short start-up time enabled" />
                <bit offset="5" name="SPIEN" text="Serial program downloading (SPI) enabled" />
            </fuse>
            <lock>
                <bit offset="1" name="Lock" index="0" />
                <bit offset="2" name="Lock" index="1" />
                <!-- lock sets description -->
                <set code="Lock=11" text="Mode 1: No memory lock features enabled" />
                <set code="Lock=10" text="Mode 2: Further programming disabled" />
                <set code="Lock=00" text="Mode 3: Further programming and verification disabled" />
            </lock>
        </descr>
        <descr list="AT90S2333,AT90S4433" >
            <fuse>
                <bit offset="0" name="CKSEL" index="0" />
                <bit offset="1" name="CKSEL" index="1" />
                <bit offset="2" name="CKSEL" index="2" />
                <bit offset="3" name="BODEN" text="Brown-out detection enabled" />
                <bit offset="4" name="/BODLEVEL" text="NOT Brown-out detection level at VCC=2.7V" />
                <bit offset="5" name="SPIEN" text="Serial program downloading (SPI) enabled" />
                <!-- fuse sets description -->
                <set code="CKSEL=101" text="Ceramic Resonator" />
                <set code="CKSEL=111" text="Ceramic Resonator; BOD Enabled or power-on reset" />
                <set code="CKSEL=110" text="Ceramic Resonator; fast rising power" />
                <set code="CKSEL=100" text="Crystal Oscillator; BOD Enabled or power-on reset" />
                <set code="CKSEL=010" text="Crystal Oscillator; default value" />
                <set code="CKSEL=011" text="Crystal Oscillator; fast rising power" />
                <set code="CKSEL=001" text="External Clock; BOD Enabled or power-on reset" />
                <set code="CKSEL=000" text="External Clock; slowly rising power" />
            </fuse>
            <lock>
                <bit offset="1" name="Lock" index="0" />
                <bit offset="2" name="Lock" index="1" />
                <!-- lock sets description -->
                <set code="Lock=11" text="Mode 1: No memory lock features enabled" />
                <set code="Lock=10" text="Mode 2: Further programming disabled" />
                <set code="Lock=00" text="Mode 3: Further programming and verification disabled" />
            </lock>
        </descr>
        <descr list="AT90S2323,AT90S8534">
            <fuse>
                <bit offset="0" name="FSTRT" text="Short start-up time enabled" />
                <bit offset="5" name="SPIEN" text="Serial program downloading (SPI) enabled" />
            </fuse>
            <lock>
                <bit offset="1" name="Lock" index="0" />
                <bit offset="2" name="Lock" index="1" />
                <!-- lock sets description -->
                <set code="Lock=11" text="Mode 1: No memory lock features enabled" />
                <set code="Lock=10" text="Mode 2: Further programming disabled" />
                <set code="Lock=00" text="Mode 3: Further programming and verification disabled" />
            </lock>
        </descr>
        <descr list="AT90S2343" >
            <fuse>
                <bit offset="0" name="/FB0" text="NOT External clock enabled" /> <!-- enabled but invert -->
                <bit offset="5" name="SPIEN" text="Serial program downloading (SPI) enabled" />
            </fuse>
            <lock>
                <bit offset="1" name="Lock" index="0" />
                <bit offset="2" name="Lock" index="1" />
                <!-- lock sets description -->
                <set code="Lock=11" text="Mode 1: No memory lock features enabled" />
                <set code="Lock=10" text="Mode 2: Further programming disabled" />
                <set code="Lock=00" text="Mode 3: Further programming and verification disabled" />
